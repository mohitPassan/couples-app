{
  "expo": {
    "name": "CouplesApp",
    "slug": "CouplesApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/new-icon.png",
    "splash": {
      "image": "./assets/drawing.png",
      "resizeMode": "contain",
      "backgroundColor": "#f33030"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.mohitpassan.couplesapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/new-icon.png",
        "backgroundColor": "#f33030"
      }
    },
    "web": {
      "favicon": "./assets/new-icon.png"
    },
    "privacy": "unlisted"
  }
}
